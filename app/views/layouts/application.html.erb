  <%= link_to "Sign in", sign_in_path %>
<% end %> </li></a>
      </ul>
    </div>
    </div>
  </nav>
  <div class="container">
     <%= yield %>
     </div>
     <%= javascript_include_tag 'application', "https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js" %>
  </body>
</html>
