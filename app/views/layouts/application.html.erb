    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
   
      <nav>
    <div class="nav-wrapper ">
    <div class="container">
      <a href="#" class="brand-logo brand-logo-extended">Jonathan Daniel Bain</a>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
        <li><a href=<%=link_to "#" %>>Developer</a></li>
        <li><a href="badges.html">Tutor</a></li>
        <li><a href="collapsible.html">Photographer</a></li>
        <li><a><% if signed_in? %>
